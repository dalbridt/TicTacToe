<VBox alignment="CENTER" prefHeight="526.0" prefWidth="460.0" spacing="20.0" xmlns="http://javafx.com/javafx/20.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="set.tictactoe.restartController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>
   <children>
      <AnchorPane prefHeight="497.0" prefWidth="438.0">
         <children>
            <Label fx:id="winnerText" alignment="CENTER" contentDisplay="CENTER" layoutX="7.0" layoutY="22.0" prefHeight="95.0" prefWidth="408.0" textAlignment="CENTER" textFill="#800000">
               <font>
                  <Font name="Kefa Regular" size="36.0" />
               </font>
            </Label>
            <Button fx:id="restartButton" layoutX="124.0" layoutY="140.0" mnemonicParsing="false" style="-fx-border-color: #790000;" text="PLAY AGAIN" textFill="#790000">
               <font>
                  <Font name="Kefa Regular" size="24.0" />
               </font>
            </Button>
            <ImageView fx:id="startPic" fitHeight="272.0" fitWidth="269.0" layoutX="76.0" layoutY="210.0" preserveRatio="true">
               <image>
                  <Image url="@../../assets/regnum_picture_1452529770_normal.png" />
